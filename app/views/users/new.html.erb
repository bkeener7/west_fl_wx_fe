<h1>New User Form</h1>
<%= form_with url: "/users", local: true, method: :post do |form| %>

  <p><%= form.label :first_name, "First name:" %>
  <%= form.text_field :first_name %>
  <%= form.label :last_name, "Last name:" %>
  <%= form.text_field :last_name %>
  <%= form.label :email, "Email:" %>
  <%= form.text_field :email %> </p>

  <%= form.check_box "plan" %>
  <%= form.label :plan %>
  <%= form.check_box "car" %>
  <%= form.label :car %>
  <%= form.check_box "house" %>
  <%= form.label :house %>
  <%= form.check_box "prep kits" %>
  <%= form.label :prep_kits %>
  <%= form.check_box "records" %>
  <%= form.label :records %>
  <%= form.check_box "pets" %>
  <%= form.label :pets %>
  <%= form.check_box "kids" %>
  <%= form.label :kids %>
  <%= form.submit "Submit"%>
<%end %>